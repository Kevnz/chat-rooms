{
  "name": "chat-rooms",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "jest --config=jest.config.js --detectOpenHandles --forceExit --watch",
    "lint": "eslint ./src",
    "watch": "npm-run-all --parallel dev:**",
    "build": "webpack --config ./src/webpack/prod.config.js --mode production",
    "dev:ui": "webpack-dev-server --config ./src/webpack/dev.config.js --mode development",
    "docker:up": "docker-compose -f ./docker/dev/docker-compose.yml up",
    "docker:down": "docker-compose -f ./docker/dev/docker-compose.yml down",
    "dev:db": "npm run docker:up",
    "dev:server": "nodemon ./src/server/index.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@brightleaf/cache": "^1.0.3",
    "@brightleaf/react-hooks": "^0.4.1",
    "@hapi/good": "^8.2.0",
    "@hapi/good-console": "^8.1.0",
    "@hapi/hapi": "^18.3.1",
    "@hapi/inert": "^5.2.0",
    "@hapi/nes": "^11.1.0",
    "@hapi/vision": "^5.5.2",
    "@reach/router": "^1.2.1",
    "apollo-server-hapi": "^2.5.0",
    "bcrypt": "^3.0.6",
    "blipp": "^4.0.0",
    "bookshelf": "^0.14.2",
    "graphql": "^14.3.0",
    "hapi-router": "^5.0.0",
    "isom": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.17.0-next3",
    "pg": "^7.11.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-form-elements": "^2.4.1",
    "universal-analytics": "^0.4.20",
    "xtconf": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@kev_nz/eslint-config": "^3.3.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.3",
    "coveralls": "^3.0.3",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.0",
    "react-hot-loader": "^4.8.5",
    "react-testing-library": "^7.0.0",
    "style-loader": "^0.23.1",
    "typesetter": "^2.0.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "@storybook/react": "^5.0.11",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addons": "^5.0.11"
  }
